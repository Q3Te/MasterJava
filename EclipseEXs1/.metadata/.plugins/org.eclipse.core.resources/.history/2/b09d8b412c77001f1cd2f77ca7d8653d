package com.masterjava.ex4;

public class Libro {
	private int isbn;
	private String titulo;
	private String autor;
	private int nPaginas;
	
//	public Libro() { //Permite incializar el objeto
//		
//	}
	
	public Libro(int isbn) {
		super();
		this.isbn=isbn;
	}

	public String getTitulo() {
		return titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}

	public String getAutor() {
		return autor;
	}

	public void setAutor(String autor) {
		this.autor = autor;
	}

	public int getnPaginas() {
		return nPaginas;
	}

	public void setnPaginas(int nPaginas) {
		this.nPaginas = nPaginas;
	}
	
	proceso_Resultado(int g_local, int g_visitante) {
		this.g_local=g_local;
		this.g_visitante=g_visitante;
		
		if(g_local>g_visitante) {
			e_local.p_Ganados(g_local);	
		}
		else if (g_visitante>g_local) {
			e_visitante.p_Ganados(g_visitante); 
		}
	}
	
}
